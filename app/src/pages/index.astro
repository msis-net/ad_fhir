---
import Layout from "@layouts/Layout.astro";
import Ajax from "@components/data_ajax.vue"
import Datatables from "@components/data_datatables.vue";
import Draggable from "@components/draggable.vue";
import Food from "@components/food.vue";
import Counter from '@components/counter.vue';
import ColorPucker from "@components/ColorPucker.vue";
const path = "."
---

<Layout title="dashboard" path = {path} page="index">
	<main class="app-main"> <!--begin::App Content Header-->
        <div class="app-content-header"> <!--begin::Container-->
            <div class="container-fluid"> <!--begin::Row-->
                <div class="row">
                    <div class="col-sm-6">
                        <h3 class="mb-0">Components</h3>
                    </div>
                    <div class="col-sm-6">
                        <ol class="breadcrumb float-sm-end">
                            <li class="breadcrumb-item"><a href="/">Home</a></li>
                            <li class="breadcrumb-item active" aria-current="page">
                                nottime
                            </li>
                            <li>
                                
                            </li>
                        </ol>
                    </div>
                </div> <!--end::Row-->
            </div> <!--end::Container-->
        </div> <!--end::App Content Header--> <!--begin::App Content-->
        <div class="app-content"> <!--begin::Container-->
            <div class="container-fluid"> <!--begin::Row-->
                <div class="row"> <!--begin::Col-->
                    <div class="col-lg-3 col-6"> <!--begin::Small Box Widget 1-->
                      <Draggable client:idle/>
                      <Ajax client:load/>
                    </div> <!--end::Col-->
                    <div class="col-lg-3 col-6"> <!--begin::Small Box Widget 2-->
                      <Food />
                    </div> <!--end::Col-->
                    <div class="col-lg-3 col-6"> <!--begin::Small Box Widget 3-->
                      <Counter client:idle/>
                    </div> <!--end::Col-->
                    <div class="col-lg-3 col-6"> <!--begin::Small Box Widget 4-->
                      <ColorPucker client:load/>
                    </div> <!--end::Col-->
                </div> <!--end::Row--> <!--begin::Row-->
                
                <div class="row">
                 
                    <div class="col-lg-12">
                      
                        
                    <Datatables client:visible/>
                    

                    </div>
                </div>
            </div> <!--end::Container-->
        </div> <!--end::App Content-->
    </main> <!--end::App Main--> <!--begin::Footer-->
</Layout>

<!-- OPTIONAL SCRIPTS -->
