---
import Layout from "@layouts/Layout.astro";
const label = 'Calendar'

import Vcalendar from "@components/vue/vcalendar.vue";

import Fullcalendar from "@components/vue/fullcalendar.vue";

---

<Layout title="layout" path = "../" page={label}>
    <main class="app-main"> <!--begin::App Content Header-->
        <div class="app-content-header"> <!--begin::Container-->
            <div class="container-fluid"> <!--begin::Row-->
                <div class="row">
                    <div class="col-sm-6">
                        <h3 class="mb-0">{label}</h3>
                    </div>
                    <div class="col-sm-6">
                        <ol class="breadcrumb float-sm-end">
                            <li class="breadcrumb-item"><a href="#">Home</a></li>
                            <li class="breadcrumb-item active" aria-current="page">
                                {label}
                            </li>
                        </ol>
                    </div>
                </div> <!--end::Row-->
            </div> <!--end::Container-->
        </div> <!--end::App Content Header--> <!--begin::App Content-->

        <div class="container-fluid">
            <div class="row g-4 mb-4">
                <div class="col-md-6">
                    <div class="card">
                        <div class="card-header">
                            <h5 class="card-title">
                                <a href="https://vcalendar.netlify.app/" target="_blank" rel="noopener noreferrer">
                                    v-calender:2024-01-20ボタンなどが動かない
                                </a>
                            </h5>
                            <div class="card-tools"> <button type="button" class="btn btn-tool" data-lte-toggle="card-collapse"> <i data-lte-icon="expand" class="bi bi-plus-lg"></i> <i data-lte-icon="collapse" class="bi bi-dash-lg"></i> </button> </div> <!-- /.card-tools -->
                        </div> <!-- /.card-header -->
                        <div class="card-body">
                            
                            <Vcalendar/>
                                        
                        
                        </div> <!-- /.card-body -->
                        <div class="card-footer">Footer</div> <!-- /.card-footer-->
                    </div> <!-- /.card -->
                </div> <!-- /.col -->
                <div class="col-md-6">
                    <div class="card">
                        <div class="card-header">
                            <h5 class="card-title">
                                <a href="https://vcalendar.netlify.app/" target="_blank" rel="noopener noreferrer">
                                    v-calender:2024-01-20
                                </a>
                            </h5>
                            <div class="card-tools"> <button type="button" class="btn btn-tool" data-lte-toggle="card-collapse"> <i data-lte-icon="expand" class="bi bi-plus-lg"></i> <i data-lte-icon="collapse" class="bi bi-dash-lg"></i> </button> </div> <!-- /.card-tools -->
                        </div> <!-- /.card-header -->
                        <div class="card-body">
                            
                            <Vcalendar 
                                            class="my-calendar"
                                            borderless
                                            transparent 
                                            show-weeknumbers
                                            >
                                            <template #footer>
                                            <div class="w-full px-4 pb-3">
                                                <button
                                                class="bg-indigo-600 hover:bg-indigo-700 text-white font-bold w-full px-3 py-1 rounded-md"
                                                @click="moveToday"
                                                >
                                                Today
                                                </button>
                                            </div>
                                            </template>
                                        </Vcalendar>
                        
                        </div> <!-- /.card-body -->
                        <div class="card-footer">Footer</div> <!-- /.card-footer-->
                    </div> <!-- /.card -->
                </div> <!-- /.col -->
            </div> <!--end::Row-->

        </div> <!--end::Container-->

        <div class="app-content"> <!--begin::Container-->
            <div class="container-fluid">
                <div class="row g-4 mb-4">
                    <div class="col-md-12">
                        <div class="card">
                            <div class="card-header">
                                <h5 class="card-title">
                                    <a href="https://github.com/fullcalendar/fullcalendar-vue" target="_blank" rel="noopener noreferrer" class="text-decoration-none">
                                        Full Calendar:2024-01-20
                                    </a>
                                </h5>
                                <div class="card-tools"> <button type="button" class="btn btn-tool" data-lte-toggle="card-collapse"> <i data-lte-icon="expand" class="bi bi-plus-lg"></i> <i data-lte-icon="collapse" class="bi bi-dash-lg"></i> </button> </div> <!-- /.card-tools -->
                            </div> <!-- /.card-header -->
                            <div class="card-body">
                                <Fullcalendar client:load/>
                                <div>
                                    <ul>
                                        <li>
                                            <a href="https://codepen.io/iw3/embed/DoZqRa?default-tab=js,result&height=265&theme-id=0" target="_blank" rel="noopener noreferrer">https://codepen.io/iw3/embed/DoZqRa?default-tab=js,result&height=265&theme-id=0</a>
                                        </li>
                                        <li>
                                            <a href="https://qiita.com/riki_ykgw/items/c3f7d27aefd136381139" target="_blank" rel="noopener noreferrer">https://qiita.com/riki_ykgw/items/c3f7d27aefd136381139</a>
                                        </li>
                                        <li>
                                            <a href="https://www.mitsue.co.jp/knowledge/blog/frontend/202012/08_0900.html" target="_blank">https://www.mitsue.co.jp/knowledge/blog/frontend/202012/08_0900.html</a>
                                        </li>
                                        <li>
                                            <a href="https://www.npmjs.com/package/fullcalendar" target="_blank" rel="noopener noreferrer">npm @fullcalendar</a>
                                        </li>
                                    </ul>
                                </div>
                            </div> <!-- /.card-body -->
                            <div class="card-footer">
                            </div> <!-- /.card-footer-->
                        </div> <!-- /.card -->
                    </div> <!-- /.col -->

                </div> <!--end::Row-->

            </div> <!--end::Container-->

            <div class="container-fluid">
                <div class="row g-4 mb-4">
                    <div class="col-md-12">
                        <div class="card">
                            <div class="card-header">
                                <h5 class="card-title">
                                    <a href="https://vcalendar.netlify.app/" target="_blank" rel="noopener noreferrer">
                                        VCalender:2024-01-20
                                    </a>
                                </h5>
                                <div class="card-tools"> <button type="button" class="btn btn-tool" data-lte-toggle="card-collapse"> <i data-lte-icon="expand" class="bi bi-plus-lg"></i> <i data-lte-icon="collapse" class="bi bi-dash-lg"></i> </button> </div> <!-- /.card-tools -->
                            </div> <!-- /.card-header -->
                            <div class="card-body">
                                
                                <Vcalendar/>   
                            
                            </div> <!-- /.card-body -->
                            <div class="card-footer">Footer</div> <!-- /.card-footer-->
                        </div> <!-- /.card -->
                    </div> <!-- /.col -->

                </div> <!--end::Row-->

            </div> <!--end::Container-->
        </div> <!--end::App Content-->

        <div class="app-content"> <!--begin::Container-->
            <div class="container-fluid"> <!--begin::Row-->

                <div class="row">
                    <div class="col-12"> <!-- Default box -->
                        <div class="card">
                            <div class="card-header">
                                <h3 class="card-title">
                                    <a href="https://vcalendar.netlify.app/" target="_blank" rel="noopener noreferrer">
                                        VCalender:2024-01-20
                                    </a>
                                </h3>
                                <div class="card-tools"> <button type="button" class="btn btn-tool" data-lte-toggle="card-collapse" title="Collapse"> <i data-lte-icon="expand" class="bi bi-plus-lg"></i> <i data-lte-icon="collapse" class="bi bi-dash-lg"></i> </button> <button type="button" class="btn btn-tool" data-lte-toggle="card-remove" title="Remove"> <i class="bi bi-x-lg"></i> </button> </div>
                            </div>
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-sm-6">
                                        <Vcalendar/>
                                        <Vcalendar 
                                            class="my-calendar"
                                            borderless
                                            transparent 
                                            show-weeknumbers
                                            >
                                            <template #footer>
                                            <div class="w-full px-4 pb-3">
                                                <button
                                                class="bg-indigo-600 hover:bg-indigo-700 text-white font-bold w-full px-3 py-1 rounded-md"
                                                @click="moveToday"
                                                >
                                                Today
                                                </button>
                                            </div>
                                            </template>
                                        </Vcalendar>
                                    </div>
                                    <div class="col-sm-6">
                                        <code>
                                        import Vcalendar from "@components/vue/vcalendar.vue";
                                            
                                        </code>
                                    </div>
                                </div> <!--end::Row-->
                            </div> <!-- /.card-body -->

                            <div class="card-footer">Footer</div> <!-- /.card-footer-->
                        </div> <!-- /.card -->
                    </div>
                </div> <!--end::Row-->

                <div class="row">
                    <div class="col-12"> <!-- Default box -->
                        <div class="card">
                            <div class="card-header">
                                <h3 class="card-title">
                                    <a href="https://vcalendar.netlify.app/" target="_blank" rel="noopener noreferrer">
                                        VCalender:2024-01-20
                                    </a>
                                </h3>
                                <div class="card-tools"> <button type="button" class="btn btn-tool" data-lte-toggle="card-collapse" title="Collapse"> <i data-lte-icon="expand" class="bi bi-plus-lg"></i> <i data-lte-icon="collapse" class="bi bi-dash-lg"></i> </button> <button type="button" class="btn btn-tool" data-lte-toggle="card-remove" title="Remove"> <i class="bi bi-x-lg"></i> </button> </div>
                            </div>
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-sm-6">
                                        <Vcalendar/>
                                    </div>
                                    <div class="col-sm-6">
                                        <code>
                                        import Vcalendar from "@components/vue/vcalendar.vue";
                                            
                                        </code>
                                    </div>
                                </div> <!--end::Row-->
                            </div> <!-- /.card-body -->
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-sm-6">
                                        <Vcalendar 
                                            class="my-calendar"
                                            borderless
                                            transparent 
                                            show-weeknumbers
                                            >
                                            <template #footer>
                                            <div class="w-full px-4 pb-3">
                                                <button
                                                class="bg-indigo-600 hover:bg-indigo-700 text-white font-bold w-full px-3 py-1 rounded-md"
                                                @click="moveToday"
                                                >
                                                Today
                                                </button>
                                            </div>
                                            </template>
                                        </Vcalendar>
                                </div>
                                <div class="col-sm-6">
                                        <code>
                                        オプションを利用
                                        枠線を消す
                                        背景を透明
                                            
                                        </code>
                                    </div>
                                </div>
                                
                            </div> <!-- /.card-body -->
                            <div class="card-footer">Footer</div> <!-- /.card-footer-->
                        </div> <!-- /.card -->
                    </div>
                </div> <!--end::Row-->
            </div> <!--end::Container-->
        </div> <!--end::App Content-->
    </main> <!--end::App Main--> <!--begin::Footer-->
</Layout>
